package telran.java47.post.model;

import java.time.LocalDateTime;
import java.util.List;
import java.util.Set;

import lombok.Singular;


public class Post {
	String id;
	String title;
	String content;
	String author;
	LocalDateTime dateCreated;
	@Singular
	Set<String> tags;
	Integer likes;
	@Singular
	List<Comment> comments;
}
